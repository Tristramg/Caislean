port 1194
proto udp
dev tun

# Change this to the server's hostname
remote your.server.com

client
ns-cert-type server
remote-cert-tls server
redirect-gateway def1

persist-key
persist-tun
nobind
comp-lzo
explicit-exit-notify

# Paste the root CA
<ca>
</ca>

# Paste the client's certificate
<cert>
</cert>

# Paste the client's private key
<key>
</key>
